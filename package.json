{
	"name": "nestjs-lru-cache",
	"version": "0.0.2",
	"description": "NestJS LRU memory cache module.",
	"publishConfig": {
		"access": "public"
	},
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"repository": "git@github.com:StimulCross/nestjs-lru-cache.git",
	"author": "Stimul Cross <stimulcross@gmail.com>",
	"license": "MIT",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsc -p ./tsconfig.build.json",
		"rebuild": "yarn clear && yarn build",
		"clear": "rimraf dist",
		"lint": "eslint --ext js,ts .",
		"lint:fix": "eslint --ext js,ts . --fix",
		"format:check": "prettier --check \"**/*.{ts,md,js,mjs,json}\"",
		"format:fix": "prettier --write \"**/*.{ts,md,js,mjs,json}\"",
		"test": "jest --config jest.config.json --runInBand",
		"test:cov": "jest --config jest.config.json --runInBand --coverage",
		"prepare": "husky install"
	},
	"engines": {
		"node": ">=8.3.0"
	},
	"dependencies": {
		"lru-cache": "7.17.0"
	},
	"peerDependencies": {
		"@nestjs/common": "*",
		"@nestjs/core": "*",
		"reflect-metadata": "^0.1.13"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.4.4",
		"@nestjs/common": "^9.3.9",
		"@nestjs/core": "^9.3.9",
		"@nestjs/platform-express": "^9.3.9",
		"@nestjs/testing": "^9.3.9",
		"@stimulcross/commitlint-config": "^1.4.1",
		"@stimulcross/eslint-config-node": "^1.4.1",
		"@stimulcross/eslint-config-typescript": "^1.4.1",
		"@stimulcross/prettier-config": "^1.4.1",
		"@types/jest": "^29.4.0",
		"@types/node": "^18.14.1",
		"@types/sizeof": "^1.0.1",
		"eslint": "^8.35.0",
		"husky": "^8.0.3",
		"jest": "^29.4.3",
		"lint-staged": "^13.1.2",
		"prettier": "^2.8.4",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^4.1.2",
		"sizeof": "^1.0.0",
		"ts-jest": "^29.0.5",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5"
	}
}
