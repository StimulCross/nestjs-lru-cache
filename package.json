{
	"name": "@stimulcross/repo-template",
	"version": "0.0.1",
	"description": "Basic template for repositories.",
	"main": "none",
	"repository": "git@github.com:StimulCross/repo-template.git",
	"author": "Stimul Cross <stimulcross@gmail.com>",
	"license": "MIT",
	"scripts": {
		"lint": "eslint --ext ts src",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"build": "tsukuru",
		"rebuild": "tsukuru --clean",
		"clean": "rimraf ./es ./lib ./tsconfig.tsbuildinfo",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.3",
		"@stimulcross/commitlint-config": "^1.3.0",
		"@stimulcross/eslint-config-node": "^1.3.0",
		"@stimulcross/eslint-config-typescript": "^1.3.0",
		"@stimulcross/prettier-config": "^1.3.0",
		"@types/node": "^18.8.5",
		"eslint": "^8.25.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"tslib": "^2.4.0",
		"tsukuru": "^0.8.0-pre.7",
		"typescript": "4.6.3"
	}
}
